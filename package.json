{
  "name": "pusakacu",
  "version": "1.0.0",
  "description": "pusakacu is an automation testing tool based on webdriver.io with cucumber",
  "main": "wdio.conf.js",
  "scripts": {
    "test-web": "npx wdio web.wdio.conf.js",
    "dev-web": "npx wdio web.dev.conf.js",
    "report": "allure generate allure-results -c && allure open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/depapp/pusakacu.git"
  },
  "keywords": [
    "webdriverio",
    "cucumber"
  ],
  "author": "@depapp",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/depapp/pusakacu/issues"
  },
  "homepage": "https://github.com/depapp/pusakacu#readme",
  "dependencies": {
    "@wdio/cli": "^7.12.2"
  },
  "devDependencies": {
    "@wdio/allure-reporter": "^7.10.1",
    "@wdio/cucumber-framework": "^7.12.2",
    "@wdio/local-runner": "^7.12.2",
    "chromedriver": "^93.0.1",
    "wdio-chromedriver-service": "^7.2.0"
  }
}
